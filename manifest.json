{
  "manifest_version": 3,
  "name": "中国科技股票监控",
  "version": "1.0",
  "description": "实时监控中国科技股票信息",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.allorigins.win/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
